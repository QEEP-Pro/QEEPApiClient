{
    "name": "qeep-pro/qeep-api-client",
    "description": "Client for QEEP-Pro API",
    "keywords": ["api", "client"],
    "license": "MIT",
    "authors": [
        {
            "name": "QEEP-Pro",
            "email": "info@qeep.pro"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "lint": "./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix ./src -v --dry-run --stop-on-violation --using-cache=no",
        "fix-style": "./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix ./src"
    },
    "require": {
        "php": ">=7.1",
        "symfony/serializer": "^4.0 | ^3.0",
        "symfony/property-access": "^4.0 | ^3.0",
        "jms/serializer": "^1.10",
        "doctrine/common": "^2.8",
        "indigophp/doctrine-annotation-autoload": "^0.1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^4.2",
        "php-mock/php-mock-prophecy": "^0.0.2",
        "php-mock/php-mock": "1.0",
        "friendsofphp/php-cs-fixer": "^2.11"
    },
    "autoload": {
        "psr-4": {
            "QEEP\\QEEPApiClient\\": "src/"
        }
    }
}
